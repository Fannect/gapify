{
   "name": "gapify",
   "description": "Command-line tool to compile Express apps into PhoneGap apps",
   "author": "Blake VanLandingham <blakevanlan@gmail.com>",
   "keywords": [
      "PhoneGap",
      "compile",
      "Express",
      "command-line"
   ],
   "version": "0.0.5",
   "engines": {
      "node": ">= 0.6.x",
      "npm": "1.1.x"
   },
   "bin": {
