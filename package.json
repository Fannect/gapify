   },
   "main": "./gapify",
   "repository": {
      "type": "git",
      "url": "git@github.com:Fannect/gapify.git"
   },
   "dependencies": {
      "coffee-script": "1.4.0",
      "jade": "0.27.7",
      "stylus": "0.30.1",
      "commander": "1.1.1",
      "underscore": "1.4.3",
      "fs-extra": "0.3.2",
      "snockets": "1.3.8"
   },
   "devDependencies": {
      "mocha": "1.7.4",
      "should": "1.2.1"
   },
   "scripts": {
      "test": "mocha --compilers coffee:coffee-script --reporter spec"
